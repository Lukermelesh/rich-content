{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://wix-rich-content/anchor-content-data-schema.json",
    "type": "object",
    "definitions": {},
    "properties": {
      "name": {
        "$id": "/properties/name",
        "type": "string",
        "title": "name",
        "default": "",
        "examples": ["wix"]
      },
      "type": {
        "enum": ["single"],
        "title": "type",
        "examples": ["single"]
      },
      "config": {
        "type": "object",
        "title": "configuration",
        "properties": {
          "size": {
            "$id": "/properties/config/properties/size",
            "enum": ["large"],
            "title": "size",
            "examples": ["large"]
          },
          "alignment": {
            "enum": ["center"],
            "title": "alignment",
            "examples": ["center"]
          }
        }
      }
    },
    "required": ["name"]
  }
  